<div id="<%= dom_id client %>">
  <p>
    <strong>Name:</strong>
    <%= client.name %>
  </p>

  <p>
    <strong>Document:</strong>
    <%= client.document %>
  </p>

  <p>
    <strong>Contact:</strong>
    <%= client.contact %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= client.address %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= client.email %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= client.age %>
  </p>

</div>
